#include<stdio.h>
#include<string.h>

int main()
{
    int i, j=0, k;
    char a[1002];
    scanf("%s", a);

    for(i=0; i<strlen(a); i++)
    {
        if((a[i]=='D'&&a[i+1]=='a'&&a[i+2]=='n'&&a[i+3]=='i'&&a[i+4]=='l')||(a[i]=='O'&&a[i+1]=='l'&&a[i+2]=='y'&&a[i+3]=='a')||(a[i]=='S'&&a[i+1]=='l'&&a[i+2]=='a'&&a[i+3]=='v'&&a[i+4]=='a')||(a[i]=='A'&&a[i+1]=='n'&&a[i+2]=='n')||(a[i]=='N'&&a[i+1]=='i'&&a[i+2]=='k'&&a[i+3]=='i'&&a[i+4]=='t'&&a[i+5]=='a'))
        {
            j++;
        }
    }

    if(j==1) printf("YES");

    else printf("NO");


}
