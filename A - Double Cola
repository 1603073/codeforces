#include<iostream>
#include<cstdio>
#include<cstring>
using namespace std;

int main()
{
    int n;
    scanf("%d",&n);
    int r=1;
    while(r*5<n)
    {
        n=n-(r*5);
        r=r*2;
    }
    string names[]={"Sheldon","Leonard","Penny","Rajesh","Howard"};
    cout<<names[(n-1)/r]<<endl;
    return 0;
}
