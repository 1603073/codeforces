#include<stdio.h>
int main()
{
    int n1,n2,k1,k2;
    scanf("%d %d %d %d",&n1,&n2,&k1,&k2);

    if(n1==n2 && n2==k1 && k1==k2 && k2==n1)
        printf("Second");
    else if(n1==n2 && k1<k2)
        printf("Second");
    else if(n1==n2 && k1==k2)
            printf("Second");
    else if(n1==n2 && n1==k1)
        printf("First");
    else if(n1==n2 && n2==k2)
        printf("Second");
    else if(n1==n2 && ((k1>k2)||(k2>k1)))
        printf("Second");
    else if(n1>n2)
        printf("First");
    else if(n1<n2)
    printf("Second");

    return 0;
}
