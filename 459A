#include<bits/stdc++.h>
using namespace std;
int main(){
long long int t,i,j,k,l,a,b,x,y;
cin >> a >> b >> x >> y;
if(a<=1 && b<=1 && x<=1 && y<=1 && a>=0 && b>=0 && x>=0 && y>=0){
    if(a==0 && b==0 && x==0 && y==1 || x==0 && y==0 && a==0 && b==1)
        cout << 1 << " " << 0 << " " << 1 << " " << 1;
    else if(a==0 && b==0 && x==1 && y==0 || x==0 && y==0 && a==1 && b==0)
        cout << 0 << " " << 1 << " " << 1 << " " << 1;
    else if(a==0 && b==0 && x==1 && y==1 || x==0 && y==0 && a==1 && b==1)
        cout << 0 << " " << 1 << " " << 1 << " " << 0;
    else if(a==0 && b==1 && x==1 && y==0 || x==0 && y==1 && a==1 && b==0)
        cout << 0 << " " << 0 << " " << 1 << " " << 1;
    else if(a==0 && b==1 && x==1 && y==1 || x==0 && y==1 && a==1 && b==1)
        cout << 0 << " " << 0 << " " << 1 << " " << 0;
    else if(a==1 && b==1 && x==1 && y==0 || x==1 && y==1 && a==1 && b==0)
        cout << 0 << " " << 0 << " " << 0 << " " << 1;
}
else
    cout << -1 << "\n" ;
}
