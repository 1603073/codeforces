#include<bits/stdc++.h>
using namespace std;
int main(){
char s[10],s1[10],s2[10],s3[10],s4[10],s5[10];
int i,j,k;
scanf("%s",s);
scanf("%s %s %s %s %s",s1,s2,s3,s4,s5);
if(s1[0]==s[0]||s2[0]==s[0]||s3[0]==s[0]||s4[0]==s[0]||s5[0]==s[0])
    printf("YES");
else if(s1[1]==s[1]||s2[1]==s[1]||s3[1]==s[1]||s4[1]==s[1]||s5[1]==s[1])
    printf("YES");
else
    printf("NO");

}
