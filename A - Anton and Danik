#include<stdio.h>
int main()
{
    int n,i,count1=0,count2=0;
    char s[100001];
    scanf("%ld",&n);
    scanf("%s",s);
    for(i=0;i<n;i++)
    {
        if(s[i]=='A')
            count1++;
        else if(s[i]=='D')
            count2++;
    }
    if(count1>count2)
        printf("Anton");
    else if(count2>count1)
        printf("Danik");
    else
        printf("Friendship");
}
