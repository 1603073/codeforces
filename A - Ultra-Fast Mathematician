#include<stdio.h>
#include<string.h>
int main()
{
    int i,len,count,temp[101];
    char s1[101],s2[101];
    scanf("%s %s",&s1,&s2);
    len=strlen(s1);
    for(i=0;i<len;i++)
    {
        if(s1[i]==s2[i]){
            count=0;
        temp[i]=count;
    }
    else
    {
        count=1;
        temp[i]=count;
    }
    }
    for(i=0;i<len;i++)
        printf("%d",temp[i]);
}
