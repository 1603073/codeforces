#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,a[1000],sum=0,j,count=0;
    cin>>n;
    for(i=0;i<7;i++)
        scanf("%d",&a[i]);
    for(j=1;!(sum>=n);j++){
    for(i=0;i<7;i++){
        sum+=a[i];
        if(sum<=n || sum>n)
            count++;
        if(sum>=n){
         printf("%d",count);
            break;}}
   if(sum!=n)
    count=0;}}
