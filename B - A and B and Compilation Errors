#include<bits/stdc++.h>
using namespace std;
int main(){
long long n,a,b,c,i,j,k,s1=0,s2=0,s3=0;
cin >> n;
for(i=1;i<=n;i++){
  cin >> a;
  s1=s1+a;
  }
for(i=1;i<n;i++){
    cin >> b;
    s2=s2+b;
}
for(i=1;i<n-1;i++){
    cin >> c;
    s3=s3+c;
}
  cout << abs(s1-s2) << "\n" << abs(s2-s3) << "\n";
}
