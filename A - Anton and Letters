#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,k,c=0,flag=0;
    char s[1000];
    scanf(" %[^\n]",s);
    for(i=97;i<=122;i++)
    {
        for(j=0;j<strlen(s);j++)
        {
            if(i==s[j]){
                c++;
                break;}
        }
    }
    printf("%d",c);
}
