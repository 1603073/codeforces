#include<bits/stdc++.h>
using namespace std;
int main(){
long long int t,i,j,k,l,r,x,y;
cin >> t;
for(i=1;i<=t;i++){
    cin >> l >> r;
    if(r%l==0)
        cout << l << " " << r << "\n";
    else if(r/l>1)
        cout << l << " " << l*2 << "\n" ;
    else
        cout << -1 << " " << -1 << "\n";
}
}
