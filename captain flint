#t = int(input())
for _ in range(int(input())):
     n = int(input())
     if(n>30):
          if((n-30)!= 6 and (n-30)!= 10 and (n-30)!= 14 ):
             print("YES")
             print(6, 10, 14, n-(6+14+10))
          else:
             print("YES")
             print(6, 10, 15, n-6-10-15)
     
     else:
          print("NO")    
