#include<bits/stdc++.h>
using namespace std;
int main(){
int i,j,k,c=0;
char a[100];
cin >> a;
if(strlen(a)>=3){
for(i=0;i<=strlen(a)-3;i++){
    if(a[i]=='Q')
    for(j=i+1;j<=strlen(a)-2;j++){
    if(a[j]=='A')
    for(k=j+1;k<=strlen(a)-1;k++){
    if(a[k]=='Q')
     c++;
    }
    }
    }
cout << c;
}
else
    cout << 0;
}
