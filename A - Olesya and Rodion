#include<iostream>
#include<cstdio>
using namespace std;

int main()
{
    int n,t,a(1);
    scanf("%d %d",&n,&t);
    if(n==1 && t==10)
        printf("-1");
    else
    {
        printf("%d",t);
        if(t==10)
            a++;
        for(a;a<n;a++)
            printf("0");
    }
    return 0;
}
